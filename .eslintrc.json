{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/standard"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "vue/no-use-v-if-with-v-for": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "globals": {
    "Vue": "readonly",
    "VueRouter": "readonly",
    "Vuex": "readonly",
    "VueI18n": "readonly",
    "echarts": "readonly"
  }
}